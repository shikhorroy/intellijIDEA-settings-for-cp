<templateSet group="CHelper">
  <template name="tcases" value="int tCases = in.readInteger();&#10;for (int cs = 1; cs &lt;= tCases; cs++) {&#10;    $INPUT$   &#10;}" description="Input test cases" toReformat="true" toShortenFQNames="true">
    <variable name="INPUT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="print" value="out.print($PARAMS$);" description="Print without newline" toReformat="true" toShortenFQNames="true">
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="println" value="out.printLine($PARAMS$);" description="Print with newline" toReformat="true" toShortenFQNames="true">
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rinteger" value="int $VAR$ = in.readInteger();" description="Input integer" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rlong" value="long $VAR$ = in.readLong();" description="Input long" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rdouble" value="double $VAR$ = in.readDouble();" description="Input double" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rline" value="String $VAR$ = in.readLine();" description="Input double" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rstring" value="String $VAR$ = in.readString();" description="Input string" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rlistinteger" value="List&lt;Integer&gt; $VAR$ = in.readIntegerList(in.readInteger());" description="Input integer list" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="issorted" value="boolean isSorted = IntStream.range(1, $LIST$.size())&#10;                        .allMatch(i -&gt; $LIST$.get(i) &gt;= $LIST$.get(i - 1));" description="Check a list of integer is sorted or not" toReformat="false" toShortenFQNames="true">
    <variable name="LIST" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>