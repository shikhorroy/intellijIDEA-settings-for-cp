<templateSet group="CHelper">
  <template name="tcases" value="int tCases = in.readInteger();&#10;for (int cs = 1; cs &lt;= tCases; cs++) {&#10;    $INPUT$   &#10;}" description="Input test cases" toReformat="true" toShortenFQNames="true">
    <variable name="INPUT" expression="" defaultValue="" alwaysStopAt="true" />
